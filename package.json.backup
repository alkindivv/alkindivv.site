{
  "name": "alkindivv-site",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "PORT=9999 next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "export": "next build && next export",
    "analyze": "ANALYZE=true npm run build",
    "find-unused": "next-unused",
    "perf": "npm run build && npm run start",
    "debug-prod": "NODE_ENV=production NODE_OPTIONS='--inspect' next dev",
    "prod-local": "npm run build && NODE_ENV=production DEBUG=* PORT=9999 next start",
    "verbose": "NODE_OPTIONS='--trace-warnings' npm run build && NODE_ENV=production DEBUG=* PORT=9999 next start",
    "clean-js": "node analyze-js.js",
    "clean-css": "node tailwind-optimize.js",
    "clean-all": "npm run clean-js && npm run build && npm run clean-css",
    "optimize": "node optimize-all.js"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "@types/nprogress": "^0.2.3",
    "clsx": "^2.1.0",
    "critters": "^0.0.25",
    "date-fns": "^3.3.1",
    "feed": "^5.1.0",
    "framer-motion": "^12.17.0",
    "gray-matter": "^4.0.3",
    "next": "14.1.1",
    "next-auth": "^4.24.11",
    "next-mdx-remote": "^4.4.1",
    "next-themes": "^0.2.1",
    "nprogress": "^0.2.0",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.0.1",
    "react-particles": "^2.12.2",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.13.0",
    "rehype-slug": "^6.0.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "remark-html": "^16.0.1",
    "rss": "^1.2.2",
    "sharp": "^0.34.2",
    "shiki": "^1.1.7",
    "tailwind-merge": "^2.2.1",
    "tsparticles-slim": "^2.12.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.1.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/rss": "^0.0.32",
    "autoprefixer": "^10.4.14",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.0",
    "glob": "^7.2.3",
    "next-unused": "^0.0.6",
    "postcss": "^8.4.21",
    "prettier": "^3.3.3",
    "purgecss": "^7.0.2",
    "tailwindcss": "^3.3.0",
    "ts-unused-exports": "^11.0.1",
    "typescript": "^5"
  }
}
