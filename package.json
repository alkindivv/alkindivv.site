    "clsx": "^2.1.0",
    "critters": "^0.0.23",
    "date-fns": "^3.3.1",
    "gray-matter": "^4.0.3",
    "lodash": "^4.17.21",
    "next": "14.1.1",
    "next-auth": "^4.24.11",
    "next-mdx-remote": "^4.4.1",
    "next-seo": "^6.6.0",
